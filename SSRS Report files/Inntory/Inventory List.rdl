  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ItemGCode">
            <Value>=Parameters!ItemGCode.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ItemCode">
            <Value>=Parameters!ItemCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT T1.ItmsGrpCod,T1.ItmsGrpNam,ItemCode,ItemName,OnHand,IsCommited,OnOrder,(SELECT sum(OINM.OutQty)  FROM OINM  where OINM.ItemCode=T0.ItemCode and OINM.Docdate&gt;= GETDATE()-180 GROUP BY OINM.itemcode)[Last 6 month consumption],
LastPurDat,LastPurPrc,(SELECT SUM(PDN1.Quantity) FROM PDN1 WHERE ItemCode=T0.ItemCode AND DocDate IN (SELECT MAX(DocDate) FROM PDN1 WHERE ItemCode=T0.ItemCode) GROUP BY ItemCode)[Last PO. Qty.]
FROM OITM T0,OITB T1 WHERE (T0.ItmsGrpCod=@ItemGCode OR @ItemGCode IS NULL)   AND T0.ItmsGrpCod=T1.ItmsGrpCod AND (T0.ItemCode=@ItemCode OR @ItemCode IS NULL)</CommandText>
      </Query>
