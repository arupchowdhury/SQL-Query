    <DataSource Name="DataSource1">
      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>Data Source=BGWSAPERP;Initial Catalog=BGW_SAP_PROD</ConnectString>
      </ConnectionProperties>
      <rd:SecurityType>DataBase</rd:SecurityType>
      <rd:DataSourceID>6904e601-302b-4fa5-958f-91ae6f197d31</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@fromdate">
            <Value>=Parameters!fromdate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Todate">
            <Value>=Parameters!Todate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>

select 
OITM.ItemCode,OITM.Itemname [Item Name],
isnull((select sum(isnull(O.Inqty,0)) -sum(isnull(O.Outqty,0)) from OINM O where O.docdate &lt; @fromdate and O.itemcode=OINM.itemcode  ),0) [Opening Quantity]



,
(select sum(OI.transvalue) 
from OINM OI where OI.docdate &lt; @fromdate and OI.itemcode=OINM.itemcode) 
[Opening Value]

,isnull((select sum(isnull(OI3.InQty,0)) from OINM OI3 where  OI3.itemcode=OINM.itemcode and OI3.docdate between @fromdate and @Todate and OI3.Transtype In (20,59)),0.00) [Purchase Qty]
,

isnull(
(select sum(isnull(OI4.transvalue,0)) from OINM OI4 where OI4.itemcode=OINM.itemcode and OI4.docdate between @fromdate and @Todate and OI4.Transtype In (20,69,18,162)),0)

 [Purchase Value]


,isnull(((select sum(isnull(O.Inqty,0)) -sum(isnull(O.Outqty,0)) from OINM O where O.docdate &lt; @fromdate and O.itemcode=OINM.itemcode) + 

(select sum(isnull(S.InQty,0)) from OINM S where  S.itemcode=OINM.itemcode and S.docdate between @fromdate and @Todate and S.Transtype In (20,59))),0.00) [Total Qty]
,


((select sum(OI.transvalue) 
from OINM OI where OI.docdate &lt; @fromdate and OI.itemcode=OINM.itemcode) 
+

isnull((select sum(isnull(OI4.transvalue,0)) from OINM OI4 where OI4.itemcode=OINM.itemcode and OI4.docdate between @fromdate and @Todate and OI4.Transtype In (20,69,18,162)),0)
) [Total Value]




,(isnull((select sum(isnull(OI1.OutQty,0)) from OINM OI1 where  OI1.itemcode=OINM.itemcode and OI1.docdate between @fromdate and @Todate and OI1.outqty&gt;0) ,0) 
-
isnull((select sum(isnull(OI1.InQty,0)) from OINM OI1 where  OI1.itemcode=OINM.itemcode 
and OI1.docdate between @fromdate and @Todate and OI1.Inqty&gt;0 and OI1.Transtype=60) ,0)
)

[Comsumption Quantity]




,(isnull((select sum(isnull(OI2.Calcprice,0)*isnull(OI2.OutQty,0)) from OINM OI2 where  OI2.itemcode=OINM.itemcode and OI2.docdate between @fromdate and @Todate ),0)

-
isnull((select sum(isnull(OI2.Calcprice,0)*isnull(OI2.InQty,0)) from OINM OI2 where 
 OI2.itemcode=OINM.itemcode and OI2.docdate between @fromdate and @Todate and OI2.Transtype=60),0)
)

 [Comsumption Value]





,(isnull((select sum(isnull(O.Inqty,0)) -sum(isnull(O.Outqty,0)) from OINM O where O.docdate &lt; @fromdate and O.itemcode=OINM.itemcode),0) + 
isnull((select sum(isnull(OO.InQty,0)) from OINM OO where  OO.itemcode=OINM.itemcode and OO.docdate between @fromdate and @Todate and OO.Transtype In (20,59)),0)) -
isnull((select sum(isnull(OI1.OutQty,0)) from OINM OI1 where  OI1.itemcode=OINM.itemcode and OI1.docdate between @fromdate and @Todate and OI1.outqty&gt;0),0) 

+

isnull((select sum(isnull(OI1.InQty,0)) from OINM OI1 where  OI1.itemcode=OINM.itemcode 
and OI1.docdate between @fromdate and @Todate and OI1.Inqty&gt;0 and OI1.Transtype=60) ,0)
 [Closing Qty]




,

((select sum(OI.transvalue) 
from OINM OI where OI.docdate &lt; @fromdate and OI.itemcode=OINM.itemcode) 
+

isnull((select sum(isnull(OI4.transvalue,0)) from OINM OI4 where OI4.itemcode=OINM.itemcode and OI4.docdate between @fromdate and @Todate and OI4.Transtype In (20,69,18,162)),0)
)

-
(isnull((select sum(isnull(OI2.Calcprice,0)*isnull(OI2.OutQty,0)) from OINM OI2 where  OI2.itemcode=OINM.itemcode and OI2.docdate between @fromdate and @Todate ),0)

-
isnull((select sum(isnull(OI2.Calcprice,0)*isnull(OI2.InQty,0)) from OINM OI2 where 
 OI2.itemcode=OINM.itemcode and OI2.docdate between @fromdate and @Todate and OI2.Transtype=60),0)
)
 [Closing Value]



from
OINM
INNER JOIN OITM ON OITM.ITEMCODE=OINM.ITEMCODE
INNER JOIN OITB ON OITB.ITMSGRPCOD=OITM.ITMSGRPCOD AND OITM.Itmsgrpcod=127

where
docdate between @fromdate and @Todate
GROUP by OITM.Itemcode,OITM.Itemname,OINM.Itemcode
order by OITM.Itemcode</CommandText>
