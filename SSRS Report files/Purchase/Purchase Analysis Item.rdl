            <Value>=Parameters!VendorType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT NULL CardCode, 'ALL' CardName, 'ALL' VendorType
UNION ALL
SELECT CardCode, CardName,VendorType FROM
(
