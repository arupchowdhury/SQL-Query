  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@TransType">
            <Value>=Parameters!TransType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FromDate">
            <Value>=Parameters!FromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToDate">
            <Value>=Parameters!ToDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ItemCode">
            <Value>=Parameters!ItemCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>IF @TransType=1
BEGIN
	SELECT T2.ItmsGrpCod,CASE WHEN DocType='S' THEN 'Service' ELSE T3.ItmsGrpNam END ItmsGrpNam,CASE WHEN DocType='S' THEN T1.Dscription ELSE T2.FrgnName END [Sub-Group],T0.DocNum,T0.DocDate,CASE WHEN T0.CardCode LIKE '%IMP%' THEN 'Import' ELSE 'Local' END [Supplier Type],T0.CardCode,T0.CardName,T0.DocType,
	CASE WHEN DocType='S' THEN T1.U_SCode ELSE T1.ItemCode END ItemCode,T1.Dscription,CASE WHEN DocType='S' THEN T1.U_Remark ELSE T1.Text END [Details],CASE WHEN DocType='S' THEN T1.U_SUOM ELSE T1.unitMsr END unitMsr,CASE WHEN DocType='S' THEN T1.U_Quanity ELSE T1.Quantity END [Vol.],T1.Price [Unit Price],ISNULL(T1.LineVat,0) Vat,
	ISNULL(LineTotal,0)[Val.],ISNULL(LineTotal,0)+ISNULL(LineVat,0) [Total Val.],T0.DocStatus,T0.U_SPR [SPR],T0.U_Condition1 [Condition1],T0.U_Condition2 [Condition2],T0.U_Condition3 [Condition3]
	FROM OPOR T0 JOIN POR1 T1 ON T1.DocEntry=T0.DocEntry
	LEFT JOIN OITM T2 ON T2.ItemCode=T1.ItemCode
	LEFT JOIN OITB T3 ON T3.ItmsGrpCod=T2.ItmsGrpCod
	WHERE T0.DocDate BETWEEN @FromDate AND @ToDate
	AND T0.CANCELED NOT IN ('Y') AND (T1.ItemCode=@ItemCode OR @ItemCode IS NULL OR T1.Dscription=@ItemCode)
END
ELSE IF @TransType=2
BEGIN
	SELECT T2.ItmsGrpCod,CASE WHEN DocType='S' THEN 'Service' ELSE T3.ItmsGrpNam END ItmsGrpNam,CASE WHEN DocType='S' THEN T1.Dscription ELSE T2.FrgnName END [Sub-Group],T0.DocNum,T0.DocDate,CASE WHEN T0.CardCode LIKE '%IMP%' THEN 'Import' ELSE 'Local' END [Supplier Type],T0.CardCode,T0.CardName,T0.DocType,
	CASE WHEN DocType='S' THEN T1.U_SCode ELSE T1.ItemCode END ItemCode,T1.Dscription,CASE WHEN DocType='S' THEN T1.U_Remark ELSE T1.Text END [Details],CASE WHEN DocType='S' THEN T1.U_SUOM ELSE T1.unitMsr END unitMsr,CASE WHEN DocType='S' THEN T1.U_Quanity ELSE T1.Quantity END [Vol.],T1.Price [Unit Price],ISNULL(T1.LineVat,0) Vat,
	ISNULL(LineTotal,0)[Val.],ISNULL(LineTotal,0)+ISNULL(LineVat,0) [Total Val.],T0.DocStatus,T0.U_SPR [SPR],T0.U_Condition1 [Condition1],T0.U_Condition2 [Condition2],T0.U_Condition3 [Condition3]
	FROM OPDN T0 JOIN PDN1 T1 ON T1.DocEntry=T0.DocEntry
	LEFT JOIN OITM T2 ON T2.ItemCode=T1.ItemCode
	LEFT JOIN OITB T3 ON T3.ItmsGrpCod=T2.ItmsGrpCod
	WHERE T0.DocDate BETWEEN @FromDate AND @ToDate
	AND T0.CANCELED NOT IN ('Y') AND (T1.ItemCode=@ItemCode OR @ItemCode IS NULL OR T1.Dscription=@ItemCode)
END
ELSE IF @TransType=3
BEGIN
	SELECT T2.ItmsGrpCod,CASE WHEN DocType='S' THEN 'Service' ELSE T3.ItmsGrpNam END ItmsGrpNam,CASE WHEN DocType='S' THEN T1.Dscription ELSE T2.FrgnName END [Sub-Group],T0.DocNum,T0.DocDate,CASE WHEN T0.CardCode LIKE '%IMP%' THEN 'Import' ELSE 'Local' END [Supplier Type],T0.CardCode,T0.CardName,T0.DocType,
	CASE WHEN DocType='S' THEN T1.U_SCode ELSE T1.ItemCode END ItemCode,T1.Dscription,CASE WHEN DocType='S' THEN T1.U_Remark ELSE T1.Text END [Details],CASE WHEN DocType='S' THEN T1.U_SUOM ELSE T1.unitMsr END unitMsr,CASE WHEN DocType='S' THEN T1.U_Quanity ELSE T1.Quantity END [Vol.],T1.Price [Unit Price],ISNULL(T1.LineVat,0) Vat,
	ISNULL(LineTotal,0)[Val.],ISNULL(LineTotal,0)+ISNULL(LineVat,0) [Total Val.],T0.DocStatus,T0.U_SPR [SPR],T0.U_Condition1 [Condition1],T0.U_Condition2 [Condition2],T0.U_Condition3 [Condition3]
	FROM OPCH T0 JOIN PCH1 T1 ON T1.DocEntry=T0.DocEntry
	LEFT JOIN OITM T2 ON T2.ItemCode=T1.ItemCode
	LEFT JOIN OITB T3 ON T3.ItmsGrpCod=T2.ItmsGrpCod
	WHERE T0.DocDate BETWEEN @FromDate AND @ToDate
	AND T0.CANCELED NOT IN ('Y') AND (T1.ItemCode=@ItemCode OR @ItemCode IS NULL OR T1.Dscription=@ItemCode)
END</CommandText>
      </Query>
