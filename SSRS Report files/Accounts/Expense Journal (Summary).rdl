  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FromDate">
            <Value>=Parameters!FromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToDate">
            <Value>=Parameters!ToDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @TempExpense TABLE
(
    Account VARCHAR(50),
    AccountName VARCHAR(100),
    BillCount VARCHAR(20),
    Total DECIMAL(18,2),
    Remarks TEXT
)

INSERT INTO @TempExpense
SELECT T2.ShortName Account,
       CASE WHEN T0.DocType='S' THEN T0.CardName ELSE (SELECT AcctName FROM OACT WHERE AcctCode=T2.ShortName) END [Account Name],
       COUNT(T0.DocNum)[Bill Count],
       SUM(T2.Debit) [Total],
       NULL
FROM OVPM T0, OJDT T1, JDT1 T2
WHERE T0.TransId=T1.TransId AND T1.TransId=T2.TransId AND T0.DocDate BETWEEN @FromDate AND @ToDate
AND T2.Debit&gt;0
GROUP BY T2.ShortName, T0.DocType, T0.CardName

-- Update remarks by concatenating all remarks for each account
UPDATE TE
SET Remarks = CONCAT_Remarks.ConcatenatedRemarks
FROM @TempExpense TE
INNER JOIN (
    SELECT 
        T2.ShortName AS Account,
        STRING_AGG(ISNULL(T1.U_lrlnmemo, ''), ' || ') AS ConcatenatedRemarks
    FROM OVPM T0
    INNER JOIN OJDT T1 ON T0.TransId = T1.TransId
    INNER JOIN JDT1 T2 ON T1.TransId = T2.TransId
    WHERE T0.DocDate BETWEEN @FromDate AND @ToDate
    AND T2.Debit &gt; 0
    GROUP BY T2.ShortName
) CONCAT_Remarks ON TE.Account = CONCAT_Remarks.Account

SELECT Account,AccountName [Account Name],BillCount [Bill Count],Total [Total],Remarks  FROM @TempExpense</CommandText>
      </Query>
