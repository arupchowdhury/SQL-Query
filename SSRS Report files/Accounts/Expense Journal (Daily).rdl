  <DataSets>
    <DataSet Name="ExpenseJournal">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FromDate">
            <Value>=Parameters!FromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToDate">
            <Value>=Parameters!ToDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT T0.DocDate RefDate,T1.BatchNum,T0.TransId,T1.TransType,T1.BaseRef,T1.Memo,T1.U_lrlnmemo,T2.ShortName Account,CASE WHEN T0.DocType='S' THEN T0.CardName ELSE (SELECT AcctName FROM OACT WHERE AcctCode=T2.ShortName) END [Account Name],T2.Debit [Total]  FROM OVPM T0, OJDT T1,JDT1 T2
WHERE T0.TransId=T1.TransId AND T1.TransId=T2.TransId AND T0.DocDate BETWEEN @FromDate AND @ToDate
AND T2.Debit&gt;0</CommandText>
      </Query>
