  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FromDate">
            <Value>=Parameters!FromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select OINV.CardCode,OINV.CardName,ISNULL(OCRD.U_SubUnit,'N/A') AS [Sub Unit],sum(DocTotalSy) DoctotalSy   from OINV,OCRD where docdate between @FromDate and @EndDate AND OINV.CardCode=OCRD.CardCode
 and convert(float,replace(U_Balance,',','') ) &gt;0.00
 group by OINV.CardName,OINV.CardCode ,OCRD.U_SubUnit

    union all

 Select OINV.CardCode,OINV.CardName,ISNULL(OCRD.U_SubUnit,'N/A') AS [Sub Unit],(Sum(DocTotalSy)+sum(convert(float,replace(U_Balance,',','')) ))DoctotalSy  
   from OINV,OCRD where docdate between @FromDate and @EndDate AND OINV.CardCode=OCRD.CardCode
 and (DocTotalSy+convert(float,replace(U_Balance,',','') )) &gt;convert(float,replace(U_Balance,',','') )
 and U_Balance like '-%' and (DocTotalSy+convert(float,replace(U_Balance,',','') ))&gt;0
  group by OINV.CardName,OINV.CardCode,OCRD.U_SubUnit</CommandText>
      </Query>
