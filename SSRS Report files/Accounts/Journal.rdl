  <DataSets>
    <DataSet Name="JournalEntry">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@TransId">
            <Value>=Parameters!TransId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT T0.TransId,CAST(T0.Number AS VARCHAR)Number,T0.RefDate,T0.Memo,T0.U_lrlnmemo,T1.ShortName Account,
CASE WHEN T1.ShortName LIKE '%LV%' OR T1.ShortName LIKE '%IMP%' THEN (SELECT CardName FROM OCRD WHERE CardCode=T1.ShortName) ELSE (SELECT AcctName FROM OACT WHERE AcctCode=T1.ShortName) END [Account Name],T1.Debit,T1.Credit FROM OJDT T0,JDT1 T1
WHERE T0.TransId=T1.TransId AND T0.TransId=@TransId</CommandText>
      </Query>
