  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FromDate">
            <Value>=Parameters!FromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ItmsGCode">
            <Value>=Parameters!ItmsGCode.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToDate">
            <Value>=Parameters!ToDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT ROW_NUMBER()OVER(ORDER BY DocDate,DocTime)SI,ItmsGrpCod,ItmsGrpNam,ItemCode,ItemName,DocDate,TIMEFROMPARTS(DocTime / 100, DocTime % 100, 0, 0, 0)DocTime,TransType,JrnlMemo,OB,[Prod.],Delivery,CASE WHEN JrnlMemo='OB' THEN NULL ELSE 'Q' + DATENAME(QUARTER,DocDate) END [Quarter Name],CASE WHEN JrnlMemo='OB' THEN NULL ELSE DATENAME(QUARTER,DocDate) END [Quarter No] FROM 
(
	SELECT T1.ItmsGrpCod,T2.ItmsGrpNam,T0.ItemCode,T1.ItemName,@FromDate DocDate,'' DocTime,NULL TransType,'OB' JrnlMemo,SUM(T0.InQty-T0.OutQty) OB,0 [Prod.],0 [Delivery]
	FROM OINM T0,OITM T1, OITB T2 
	WHERE T0.ItemCode=T1.ItemCode 
	AND T1.ItmsGrpCod=T2.ItmsGrpCod 
	AND DocDate &lt; @FromDate
	AND T1.ItmsGrpCod=@ItmsGCode
	GROUP BY T1.ItmsGrpCod,T2.ItmsGrpNam,T0.ItemCode,T1.ItemName

	UNION ALL
	SELECT T1.ItmsGrpCod,T2.ItmsGrpNam,T0.ItemCode,T1.ItemName,T0.DocDate,T0.DocTime,T0.TransType,JrnlMemo,0 OB,T0.InQty [Prod.],0 [Delivery]
	FROM OINM T0,OITM T1, OITB T2 
	WHERE T0.ItemCode=T1.ItemCode 
	AND T1.ItmsGrpCod=T2.ItmsGrpCod 
	AND DocDate BETWEEN @FromDate AND @ToDate 
	AND T1.ItmsGrpCod=@ItmsGCode
	AND T0.TransType=59
	UNION ALL
	SELECT T1.ItmsGrpCod,T2.ItmsGrpNam,T0.ItemCode,T1.ItemName,T0.DocDate,T0.DocTime,T0.TransType,JrnlMemo,0 OB,-T0.OutQty [Prod.],0 [Delivery]
	FROM OINM T0,OITM T1, OITB T2 
	WHERE T0.ItemCode=T1.ItemCode 
	AND T1.ItmsGrpCod=T2.ItmsGrpCod 
	AND DocDate BETWEEN @FromDate AND @ToDate 
	AND T1.ItmsGrpCod=@ItmsGCode
	AND T0.TransType=60
	UNION ALL
	SELECT T1.ItmsGrpCod,T2.ItmsGrpNam,T0.ItemCode,T1.ItemName,T0.DocDate,T0.DocTime,T0.TransType,JrnlMemo,0 OB,0 [Prod.],T0.OutQty [Delivery]
	FROM OINM T0,OITM T1, OITB T2 
	WHERE T0.ItemCode=T1.ItemCode 
	AND T1.ItmsGrpCod=T2.ItmsGrpCod 
	AND DocDate BETWEEN @FromDate AND @ToDate 
	AND T1.ItmsGrpCod=@ItmsGCode
	AND T0.TransType=15
	UNION ALL
	SELECT T1.ItmsGrpCod,T2.ItmsGrpNam,T0.ItemCode,T1.ItemName,T0.DocDate,T0.DocTime,T0.TransType,JrnlMemo,0 OB,0 [Prod.],-T0.InQty [Delivery]
	FROM OINM T0,OITM T1, OITB T2 
	WHERE T0.ItemCode=T1.ItemCode 
	AND T1.ItmsGrpCod=T2.ItmsGrpCod 
	AND DocDate BETWEEN @FromDate AND @ToDate 
	AND T1.ItmsGrpCod=@ItmsGCode
	AND T0.TransType=16
)T0
</CommandText>
      </Query>
