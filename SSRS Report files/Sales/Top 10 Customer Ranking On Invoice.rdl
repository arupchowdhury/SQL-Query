  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <Timeout>6</Timeout>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@fromdate">
            <Value>=Parameters!fromdate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@todate">
            <Value>=Parameters!todate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT  A.* FROM
(

SELECT  Top 10 T0.[Cardname],T3.ItmsGrpNam,sum(T1.[LineTotal] ) AS [Original Amount],
 sum(T1.[LineTotal]-T0.DiscSum ) AS [Invoice Amount],
sum(T1.[LineTotal]*0.15) AS VAT, ( select  sum(T1.[LineTotal])   FROM OINV T0  INNER JOIN INV1 T1 ON 
T0.DocEntry = T1.DocEntry WHERE T0.[DocDate] between @fromdate and @todate  ) as Total  FROM OINV T0 , 
INV1 T1,OITB T3,OITM t4 where T0.DocEntry = T1.DocEntry and T0.[DocDate] between @fromdate and @todate 
and t4.itmsgrpcod=T3.itmsgrpcod and T4.itemcode=T1.itemcode
 and t1.WhsCode in ('Bottles','Fur-3') 
 GROUP BY T0.[Cardname] , T3.ItmsGrpNam
order by  [Invoice Amount] desc
 
 Union all

 SELECT  Top 10 T0.[Cardname],T3.ItmsGrpNam, sum(T1.[LineTotal] ) AS [Original Amount],
 sum(T1.[LineTotal]-T0.DiscSum ) AS [Invoice Amount],
sum(T1.[LineTotal]*0.15) AS VAT, ( select  sum(T1.[LineTotal])   FROM OINV T0  INNER JOIN INV1 T1 ON 
T0.DocEntry = T1.DocEntry WHERE T0.[DocDate] between @fromdate and @todate    ) as Total  FROM OINV T0 , 
INV1 T1,OITB T3,OITM t4 where T0.DocEntry = T1.DocEntry and T0.[DocDate] between @fromdate and @todate and 
t4.itmsgrpcod=T3.itmsgrpcod and T4.itemcode=T1.itemcode
 and t1.WhsCode ='Shells'
GROUP BY T0.[Cardname],T3.ItmsGrpNam
order by  [Invoice Amount] desc
 Union all

SELECT  Top 10 T0.[Cardname], T3.ItmsGrpNam,sum(T1.[LineTotal] ) AS [Original Amount],
 sum(T1.[LineTotal]-T0.DiscSum ) AS [Invoice Amount],
sum(T1.[LineTotal]*0.15) AS VAT, ( select  sum(T1.[LineTotal])   FROM OINV T0  INNER JOIN INV1 T1 ON 
T0.DocEntry = T1.DocEntry WHERE T0.[DocDate] between @fromdate and @todate    ) as Total  FROM OINV T0 , 
INV1 T1,OITB T3,OITM t4 where T0.DocEntry = T1.DocEntry and T0.[DocDate] between @fromdate and @todate and 
t4.itmsgrpcod=T3.itmsgrpcod and T4.itemcode=T1.itemcode
 and t1.WhsCode ='Flint-F3'
GROUP BY T0.[Cardname] ,T3.ItmsGrpNam
order by  [Invoice Amount] desc
)
A 

order by ItmsGrpNam,A.[Invoice AMOUNT] desc</CommandText>
      </Query>
