  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@fy">
            <Value>=Parameters!fy.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Group">
            <Value>=Parameters!Group.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ItemCode">
            <Value>=Parameters!ItemCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @id INT
SELECT @id=AbsEntry FROM OFPR WHERE Code=@fy

DECLARE @FromDate_1 DATETIME,@FromDate_2 DATETIME,@ToDate_1 DATETIME,@ToDate_2 DATETIME,@Fy1Code VARCHAR(10),@Fy2Code VARCHAR(10)
DECLARE @Fy1 VARCHAR(10),@Fy2 VARCHAR(10)

SELECT @FromDate_1=F_RefDate,@ToDate_1=T_RefDate,@Fy1=Name,@Fy1Code=Code FROM OFPR WHERE AbsEntry=@id
SELECT @FromDate_2=F_RefDate,@ToDate_2=T_RefDate,@Fy2=Name,@Fy2Code=Code FROM OFPR WHERE AbsEntry=@id-1

SELECT  ROW_NUMBER()OVER( PARTITION BY 
            CASE 
                WHEN T0.[Month No.] &gt;= 7 THEN T0.[Month No.] - 6
                ELSE T0.[Month No.] + 6
            END ORDER BY T0.ItemCode) [SI2], DENSE_RANK()OVER(ORDER BY CASE 
                WHEN T0.[Month No.] &gt;= 7 THEN T0.[Month No.] - 6
                ELSE T0.[Month No.] + 6 END)  SI, T0.ItmsGrpCod,T0.ItmsGrpNam,T0.ItemCode,T0.ItemName,SUM(T0.Fy1Qty)Fy1Qty,SUM(T0.Fy2Qty)Fy2Qty,SUM(T0.Fy1Value)Fy1Value,SUM(T0.Fy2Value)Fy2Value,T0.FY1,T0.FY2,T0.[C. Year],T0.[Month No.],T0.[Month Name],T0.FY1Code,T0.Fy2Code FROM
(
SELECT T1.ItmsGrpCod,T2.ItmsGrpNam,T0.ItemCode,T1.ItemName,T0.Quantity [Fy1Qty],T0.LineTotal [Fy1Value],
(SELECT Name FROM OFPR WHERE AbsEntry=@id)[FY1],(SELECT Code FROM OFPR WHERE AbsEntry=@id)[FY1Code],0 [Fy2Qty],0 [Fy2Value],(SELECT Name FROM OFPR WHERE AbsEntry=@id-1)[Fy2],(SELECT Code FROM OFPR WHERE AbsEntry=@id-1)[Fy2Code],YEAR(T0.DocDate)[C. Year],MONTH(T0.DocDate)[Month No.],DATENAME(MM,T0.DocDate)[Month Name] FROM INV1 T0, OITM T1,OITB T2
WHERE T0.ItemCode=T1.ItemCode AND T1.ItmsGrpCod=T2.ItmsGrpCod AND T0.DocDate BETWEEN @FromDate_1 AND @ToDate_1 AND T2.ItmsGrpNam=@Group AND T0.ItemCode=@ItemCode
UNION ALL
SELECT T1.ItmsGrpCod,T2.ItmsGrpNam,T0.ItemCode,T1.ItemName,0 Quantity,0 LineTotal,
(SELECT Name FROM OFPR WHERE AbsEntry=@id)[FY],(SELECT Code FROM OFPR WHERE AbsEntry=@id)[FY1Code],T0.Quantity [Fy2Qty],T0.LineTotal [Fy2Value],(SELECT Name FROM OFPR WHERE AbsEntry=@id-1)[Fy2],(SELECT Code FROM OFPR WHERE AbsEntry=@id-1)[Fy2Code],YEAR(T0.DocDate)[C. Year],MONTH(T0.DocDate)[Month No.],DATENAME(MM,T0.DocDate)[Month Name] FROM INV1 T0, OITM T1,OITB T2
WHERE T0.ItemCode=T1.ItemCode AND T1.ItmsGrpCod=T2.ItmsGrpCod AND T0.DocDate BETWEEN @FromDate_2 AND @ToDate_2 AND T2.ItmsGrpNam=@Group AND T0.ItemCode=@ItemCode
)T0
GROUP BY T0.ItmsGrpCod,T0.ItmsGrpNam,T0.ItemCode,T0.ItemName,T0.FY1,T0.FY2,T0.[C. Year],T0.[Month No.],T0.[Month Name],T0.FY1Code,Fy2Code
---ORDER BY T0.[C. Year],T0.[Month No.]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
