  <DataSets>
    <DataSet Name="DSItemSalesSummaryFromBegining">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@fy">
            <Value>=Parameters!fy.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Group">
            <Value>=Parameters!Group.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ItemCode">
            <Value>=Parameters!ItemCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @FromDate_1 DATETIME,@FromDate_2 DATETIME,@ToDate_1 DATETIME,@ToDate_2 DATETIME,@GroupCode INT

SELECT @FromDate_1=MIN(F_RefDate) FROM
(
	SELECT TOP 10 * FROM OFPR ORDER BY AbsEntry DESC
)T0
----SELECT TOP 10 @FromDate_1=MIN(F_RefDate) FROM OFPR ORDER BY AbsEntry DESC
SELECT @ToDate_2=T_RefDate FROM OFPR WHERE Code=@fy

SELECT @GroupCode=ItmsGrpCod FROM OITB WHERE ItmsGrpNam=@Group


SELECT T0.ItmsGrpCod,T0.ItmsGrpNam,T0.ItemCode,T0.ItemName,T0.FY,T0.[C. Year],T0.[Month no.],T0.[Month Name],SUM(T0.Quantity)[Volume],SUM(T0.LineTotal)[Value] FROM
(
SELECT T2.ItmsGrpCod,T3.ItmsGrpNam,T1.ItemCode,T2.ItemName,(SELECT Name FROM OFPR WHERE T0.DocDate BETWEEN F_RefDate AND T_RefDate)[FY],
MONTH(T0.DocDate)[Month no.],DATENAME(MM,T0.DocDate)[Month Name],YEAR(T0.DocDate)[C. Year],T1.Quantity,T1.LineTotal FROM OINV T0, INV1 T1, OITM T2, OITB T3
WHERE T0.DocEntry=T1.DocEntry AND T1.ItemCode=T2.ItemCode AND T2.ItmsGrpCod=T3.ItmsGrpCod
AND T3.ItmsGrpNam=@Group AND T0.DocDate BETWEEN @FromDate_1 AND @ToDate_2 AND T1.ItemCode=@ItemCode
)T0
GROUP BY T0.ItmsGrpCod,T0.ItmsGrpNam,T0.ItemCode,T0.ItemName,T0.FY,T0.[C. Year],T0.[Month no.],T0.[Month Name]
ORDER BY T0.[C. Year],T0.[Month no.]</CommandText>
      </Query>
