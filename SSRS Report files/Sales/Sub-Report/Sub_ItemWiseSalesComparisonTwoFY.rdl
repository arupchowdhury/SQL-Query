        <Field Name="Fy2Value">
          <DataField>Fy2Value</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="FY1">
          <DataField>FY1</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FY2">
          <DataField>FY2</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="C__Year">
          <DataField>C. Year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Month_No_">
          <DataField>Month No.</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Month_Name">
          <DataField>Month Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Group">
            <Value>=Parameters!Group.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT ItemCode,ItemName FROM OITM WHERE ItmsGrpCod=(SELECT ItmsGrpCod FROM OITB WHERE ItmsGrpNam=@Group)</CommandText>
      </Query>
