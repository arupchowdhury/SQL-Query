  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@fromdate">
            <Value>=Parameters!fromdate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@todate">
            <Value>=Parameters!todate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Item">
            <Value>=Parameters!Item.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT T0.[Cardcode], T0.[Cardname],  sum(T1.[Quantity]) as Quantity,sum(T1.[LineTotal]) AS AMOUNT,sum(T1.[LineTotal]*0.15) AS VAT, ( select  sum(T1.[LineTotal])   FROM OINV T0  INNER JOIN INV1 T1 ON T0.DocEntry = T1.DocEntry WHERE T0.[DocDate] between @fromdate and @todate and t1.[Dscription]=@Item ) as Total  FROM OINV T0  INNER JOIN INV1 T1 ON T0.DocEntry = T1.DocEntry WHERE T0.[DocDate] between @fromdate and @todate and t1.[Dscription]=@Item GROUP BY T0.[cardCode], T0.[Cardname] order by  Amount desc</CommandText>
      </Query>
